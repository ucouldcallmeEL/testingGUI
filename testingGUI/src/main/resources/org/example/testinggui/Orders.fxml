<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<ScrollPane prefHeight="372.0" prefWidth="302.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.OrdersController">
   <content>
      <VBox prefHeight="410.0" prefWidth="284.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="42.0" prefWidth="356.0">
               <children>
                  <Button fx:id="HomeButton" alignment="CENTER" mnemonicParsing="false" onAction="#handleHomeButton" prefWidth="51.0" text="Home">
                     <HBox.margin>
                        <Insets left="5.0" />
                     </HBox.margin>
                  </Button>
                  <Label contentDisplay="CENTER" prefHeight="25.6" prefWidth="124.0" text="Orders" textAlignment="CENTER">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="5.0" />
                     </HBox.margin>
                     <padding>
                        <Insets left="5.0" />
                     </padding>
                  </Label>
                  <Button fx:id="ProfileButotn" alignment="CENTER_RIGHT" contentDisplay="RIGHT" mnemonicParsing="false" onAction="#handleProfileButton" text="Profile">
                     <HBox.margin>
                        <Insets left="40.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <VBox fx:id="orderContainer" prefHeight="369.0" prefWidth="330.0" />
         </children>
      </VBox>
   </content>
</ScrollPane>
