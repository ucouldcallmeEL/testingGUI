<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" fillWidth="false" prefHeight="278.0" prefWidth="273.0" spacing="10.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.LogInController">
   <children>
      <Label text="Log In">
         <font>
            <Font size="18.0" />
         </font></Label>
      <TextField fx:id="usernameField" promptText="Username" />
      <PasswordField fx:id="passwordField" promptText="Password" />
      <Label fx:id="SignInError" textFill="#b40000" />
      <Button fx:id="SignInButton" mnemonicParsing="false" onAction="#handleSignInButton" prefHeight="28.0" prefWidth="61.0" text="Sign In" />
      <Separator prefWidth="200.0" />
      <Label text="New with us?" />
      <Button fx:id="SignUpButton" mnemonicParsing="false" onAction="#handleSignUpButton" prefHeight="28.0" prefWidth="61.0" text="Sign Up" />
   </children>
</VBox>
